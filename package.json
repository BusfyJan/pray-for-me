{
    "name": "pray-for-me",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "firebase": "^4.8.1",
        "material-ui": "^1.0.0-beta.24",
        "material-ui-icons": "^1.0.0-beta.17",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-intl": "^2.4.0",
        "react-redux": "^5.0.6",
        "react-scripts": "1.0.17",
        "redux": "^3.7.2",
        "styled-components": "^2.3.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "translate": "npm run translate:cleanup && npm run translate:extract && npm run translate:prepare",
        "translate:cleanup": "NODE_ENV=production ./node_modules/.bin/babel-node src/i18n/cleanup.js",
        "translate:extract": "NODE_ENV=production babel ./src >/dev/null",
        "translate:prepare": "NODE_ENV=production ./node_modules/.bin/babel-node src/i18n/translator.js"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-plugin-react-intl": "^2.3.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-react-app": "^3.1.0",
        "babel-preset-stage-0": "^6.24.1",
        "rmdir": "^1.2.0"
    }
}
